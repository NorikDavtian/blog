ul.nav.nav-list
    each value, key in site.data.menu.header
        li.nav-list-item
            - var re = /^(http|https):\/\/*/gi;
            - var tar = re.test(value) ? "_blank" : "_self"
            a.nav-list-link(class={active: is_home() && key === "home"} href=value target=tar)
                != key.toUpperCase()